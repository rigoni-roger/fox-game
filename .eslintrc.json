{
  "extends": [
    "eslint:recommended",
    "plugins:import/errors",
    "prettier"
  ],
  "rules": {
    "no-console": "warn"
  },
  "plugins": ["import"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}
